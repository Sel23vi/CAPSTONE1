    "# to update channel information every 24hrs for a year. \n",
    "def pro11_data(channel_id):\n",
    "    request = youtube.channels().list(\n",
    "        part=\"snippet,contentDetails,statistics,status\",\n",
    "        id=str(channel_id))\n",
    "    response = request.execute() \n",
    "    \n",
    "    channel_id=response['items'][0]['id'],\n",
    "    channel_views=response['items'][0]['statistics']['viewCount'],\n",
    "    videos_count=response['items'][0]['statistics']['videoCount']\n",
    "    channel_subscribers_count=response['items'][0]['statistics']['subscriberCount']\n",
    "\n",
    "counter = 0\n",
    "while True:\n",
    "    if counter>365:\n",
    "        break\n",
    "    counter+=1\n",
    "    channels_list=[('channel1_id'),('channel2_id'),('channel3_id'),('channel4_id'),('channel5_id'),('channel6_id'),('channel7_id'),('channel8_id'),('channel9_id'),('channel10_id')]\n",
    "    a=len(channels_list)\n",
    "    for i in range (0,a):\n",
    "        pro11_data(channels_list[i])\n",
    "        sql=(f\"update channels set channel_views={channel_views} where channel_id='{channel_id}';\n",
    "        update channels set videos_count={videos_count} where channel_id='{channel_id}';\n",
    "        update channels set channel_subscribers_count={channel_subscribers_count} where channel_id='{channel_id}';\n",
    "        \")\n",
    "        cursor.execute(sql,x)\n",
    "    time.sleep(86400)"
   ]
