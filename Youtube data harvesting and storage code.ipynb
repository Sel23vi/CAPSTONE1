    "wb = openpyxl.load_workbook('VD.xlsx')\n",
    "ws=wb['Sheet1']\n",
    "duration_values=[ws.cell(row=i,column=2).value for i in range(2,408)]\n",
    "df1 = pd.DataFrame({'duration': duration_values})\n",
    "\n",
    "def duration_to_seconds(duration):\n",
    "    if duration.startswith('PT'):\n",
    "        duration = duration[2:]  # Remove 'PT' prefix\n",
    "        seconds = 0\n",
    "        if 'H' in duration:\n",
    "            hours, duration = duration.split('H')\n",
    "            a=int(hours)*3600\n",
    "            seconds += a\n",
    "        if 'M' in duration:\n",
    "            minutes, duration = duration.split('M')\n",
    "            b=int(minutes)*60\n",
    "            seconds += b\n",
    "        if 'S' in duration:\n",
    "            seconds_part = duration[:-1] if 'S' in duration else duration\n",
    "            seconds += int(seconds_part)\n",
    "        return seconds\n",
    "    else:\n",
    "        return None\n",
    "\n",
    "# Apply the function to the duration column and create a new column for seconds\n",
    "df1['duration_seconds'] = df1['duration'].apply(duration_to_seconds)\n",
    "\n",
    "# Display the DataFrame with the converted duration in seconds\n",
    "print(df1)"
   ]
