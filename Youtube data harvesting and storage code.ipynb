    "for j in range (0,a):\n",
    "    url = f\"https://www.googleapis.com/youtube/v3/search?key={API_KEY}&channelId={CHANNEL_ID}&part=snippet,id&order=date&maxResults=190\"\n",
    "    response = requests.get(url)\n",
    "    data = response.json()\n",
    "\n",
    "    df=pd.DataFrame([])\n",
    "    for item in data.get(\"items\", []):\n",
