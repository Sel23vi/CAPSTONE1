    "# to populate table channels with channel information using channel_ids. Replace channel_ids with your actual channel_ids\n",
    "def pro11_data(channel_id):\n",
    "    request = youtube.channels().list(\n",
    "        part=\"snippet,contentDetails,statistics,status\",\n",
    "        id=str(channel_id))\n",
    "    response = request.execute() \n",
    "    \n",
    "    return (response['items'][0]['id'],\n",
    "            response['items'][0]['snippet']['title'],\n",
    "            response['items'][0]['snippet']['publishedAt'],\n",
    "            response[\"items\"][0][\"status\"].get('madeForKids',False),\n",
    "            response['items'][0]['statistics']['viewCount'],\n",
    "            response['items'][0]['snippet']['description'],\n",
    "            response['items'][0]['status']['privacyStatus'],\n",
    "            response['items'][0]['statistics']['videoCount'],\n",
    "            response['items'][0]['statistics']['subscriberCount']\n",
    "            )\n",
    "channels_list=[('channel1_id'),('channel2_id'),('channel3_id'),('channel4_id'),('channel5_id'),('channel6_id'),('channel7_id'),('channel8_id'),('channel9_id'),('channel10_id')]\n",
    "a=len(channels_list)\n",
    "for i in range (0,a):\n",
    "    x=pro11_data(channels_list[i])\n",
    "    sql=\"insert into channels(channel_id,channel_name,channel_pat,Made_for_kids,channel_views,channel_description,channel_status,videos_count,channel_subscribers_count) values (%s,%s,%s,%s,%s,%s,%s,%s,%s);\"\n",
    "    cursor.execute(sql,x)\n",
    "    \n"
   ]
